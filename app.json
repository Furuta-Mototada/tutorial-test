{
  "expo": {
    "name": "Tutorial Test",
    "slug": "tutorial-test",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.mototada.tutorialtest"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.mototada.tutorialtest"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "ec8519cd-4d64-48fc-90ff-1a7be7401f1b"
      }
    },
    "owner": "mototada",
    "updates": {
      "url": "https://u.expo.dev/ec8519cd-4d64-48fc-90ff-1a7be7401f1b"
    },
    "runtimeVersion": {
      "policy": "appVersion"
    }
  }
}
